<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="recycling.buyer.dao.face.WashDao">

	<select id="selectWashList" resultType="Wash">
		SELECT COUNT(*) FROM Wash
	</select>
	
	<select id="view" resultType="Wash">
		SELECT * FROM Wash WHERE wCode = #{wCode}
	</select>


</mapper>